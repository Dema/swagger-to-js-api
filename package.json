{
  "name": "swagger-to-js-api",
  "version": "1.3.1",
  "description": "Takes a Swagger JSON API description and creates a Javascript API to call the endpoints. With Flow Types.",
  "main": "index.js",
  "bin": "bin/swagger-to-js-api.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "fix-lint": "eslint --fix .",
    "flow": "flow .",
    "format": "prettier --single-quote --trailing-comma --flow-parser --write *.js helpers/*.js bin/*.js"
  },
  "keywords": [
    "swagger",
    "javascript",
    "flowtype",
    "flow",
    "functional"
  ],
  "author": "Naman Goel",
  "license": "MIT",
  "dependencies": {
    "array-flatten": "^2.1.1",
    "babel-core": "6.18.2",
    "babel-generator": "6.19.0",
    "babel-plugin-transform-flow-strip-types": "6.18.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-types": "6.19.0",
    "browserify": "13.1.1",
    "chalk": "1.1.3",
    "command-line-args": "3.0.4",
    "command-line-usage": "3.0.8",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-react": "^6.9.0",
    "immutable": "^3.8.1",
    "lodash": "4.17.2",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.5.4"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "7.1.1",
    "babelify": "^7.3.0",
    "eslint": "3.11.1",
    "eslint-config-standard": "6.2.1",
    "eslint-plugin-promise": "3.4.0",
    "eslint-plugin-standard": "2.0.1",
    "flow-bin": "^0.38.0",
    "flow-typed": "^2.0.0",
    "openapi-flowtype-definition": "^1.0.1",
    "prettier": "^0.0.9"
  }
}
